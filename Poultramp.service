[Unit]
Description=Poultramp3
After=syslog.target
After=network.target

[Service]
Type=simple
WorkingDirectory=/home/poultramp/app/poultramp3/
User=poultramp
Group=poultramp
ExecStart=/usr/bin/python3 /home/poultramp/app/poultramp3/app.py > /home/poultramp/app/poultramp3/log/app.log

[Install]
WantedBy=multi-user.target
